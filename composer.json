{
  "name": "pyvalidator",
  "type": "validator to use yaml that can be used in php",
  "require-dev": {
    "symfony/yaml": "~2.7"
  },
  "require-dev": {
    "satooshi/php-coveralls": "dev-master",
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Vol2223\\PyaValidator\\": "src/"
    }
  },
  "bin": [
      "phpunit"
  ],
  "autoload-dev": {
    "psr-4": {
      "Vol2223\\PyaValidator\\": "tests/"
    }
  }
}
